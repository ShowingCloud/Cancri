<div id="main" class="main" data-src="<%= image_path('homepage/register-pagebk.png') %>">
  <div class="container">
    <div class="row">
      <div id="user-display" class="photo-inner">
        <div class="user-display-content" data-src="<%= image_path('homepage/user-display-inner-bk.png') %>">
          <%= image_tag(@photo.image_url) %>
          <i class="glyphicon glyphicon-save user-display-icon user-display-download"></i>
          <i class="glyphicon glyphicon-heart user-display-icon user-display-heart"></i>
          <i class="glyphicon glyphicon-remove-circle user-display-icon user-display-close"></i>
        </div>
        <div class="user-display-title" data-src="<%= image_path('homepage/user-display-inner-title-bk.png') %>">
          2016机械奥运
        </div>
      </div>
    </div>
  </div>
  <div id="footer">
    <div class="container">
      <div class="footer">
        <a>关于我们</a>
        <span class="separator"></span>
        <a>联系我们</a>
      </div>
    </div>
  </div>
</div>